{
    "manifest_version": 3,
    "name": "ABC Replacer APP",
    "version": "1.0",
    "description": "Replace words with words.",
    "content_scripts": [
        {
          "js": ["content.js"],
          "matches": [
            "<all_urls>"
          ]
        }
      ],
      "action": {
        "default_title": "Click Me",   // optional, shown in tooltip
        "default_popup": "popup/popup.html"  // optional
      }
  }